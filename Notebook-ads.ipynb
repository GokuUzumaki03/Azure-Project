                "# Game Sales"
            ],
            "metadata": {
                "azdata_cell_guid": "750cb91e-96aa-4aae-b289-19a6bf39a5ed"
            },
            "attachments": {}
        },
        {
            "cell_type": "code",
            "source": [
                "SELECT  TOP 10 YearOfRelease, ROUND(SUM(global_sales), 1) AS TotalGlobalSales\r\n",
                "FROM GameSales.dbo.VideoGamesSales\r\n",
                "WHERE Global_sales IS NOT NULL\r\n",
                "GROUP BY YearOfRelease\r\n",
                "Order By SUM(Global_sales) DESC;"
            ],
            "metadata": {
